{
  "name": "binary-coffee-dev",
  "version": "2022.10.10",
  "scripts": {
    "ng": "nx",
    "start": "ng serve --watch --poll=2000 --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:admin": "ng serve dashboard --watch --port=4201 --poll=2000 --host=0.0.0.0 --disable-host-check",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "build:admin": "ng build dashboard --configuration=dev",
    "build:admin:prod": "ng build dashboard --configuration production",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "serve:ssr": "node dist/server/main",
    "build:ssr": "ng run blog:build:production && ng run blog:server:production",
    "build:ssr:dev": "ng run blog:build:dev && ng run blog:server:dev"
  },
  "private": true,
  "dependencies": {

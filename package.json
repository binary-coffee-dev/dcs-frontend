{
  "name": "dcs-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "npm run get_env && ng",
    "start": "npm run get_env && ng serve --watch --poll=2000 --host=0.0.0.0",
    "build": "npm run get_env && ng build",
    "build.prod": "npm run get_env && ng build --prod",
    "test": "npm run get_env && jest --coverage",
    "lint": "npm run get_env && ng lint",
    "e2e": "npm run get_env && ng e2e",
    "get_env": "node ./node_modules/ng-node-environment/index.js",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "npm run get_env && ng build --prod && ng run dcs-frontend:server:production --bundleDependencies all",
    "check": "npm run test && npm run lint && npm run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@nguniversal/express-engine": "v8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "@ngxs/store": "^3.5.1",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "express": "^4.15.2",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "ng-lazyload-image": "^7.0.1",
    "ng-node-environment": "^1.1.1",
    "ngx-markdown": "^8.2.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.23",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/{main.ts,polyfills.ts,index.ts}",
      "!src/**/*{.module.ts,.model.ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ]
  }
}
